class Student:
    # [assignment] Skeleton class. Add your code here

    def __init__(self, name, age, tracks, score):
        self.name = name
        print("Student name: ", name)
        self.age = age
        print("Student age: ", age)
        self.tracks = tracks
        print("Student tracks: ", tracks)
        self.score = score
        print("Student score: ", score)

    def change_name(self, name):
        self.name = name
        print("Student's new name is: ", self.name)

    def change_age(self, age):
        self.age = age
        print("Student's new age is: ", self.age)

    def add_track(self, tracks):
        self.tracks = tracks
        print("Student's new track is: ", self.tracks)

    def get_score(self):
        print("Student's score is: ", self.score)


Bob = Student(name="Bob", age=26, tracks=["FE", "BE"], score=20.90)

# Expected methods
Bob.change_name("Peter")
Bob.change_age(34)
Bob.add_track("UI/UX")
Bob.get_score()
